[
  {
    "database": "fastapi_treafik",
    "index": "individuals_index",
    "setting": {
      "analysis": {
        "analyzer": {
          "vi_analyzer": {
            "filter": [
              "lowercase"
            ],
            "type": "custom",
            "tokenizer": "vi_tokenizer"
          }
        },
        "tokenizer": {
          "vi_tokenizer": {
            "token_chars": [
              "letter",
              "digit",
              "punctuation",
              "symbol"
            ],
            "min_gram": "2",
            "type": "ngram",
            "max_gram": "3"
          }
        }
      }
    },
    "nodes": {
      "table": "individuals",
      "columns": [
        "id",
        "full_name",
        "national_id",
        "citizen_id",
        "image_url",
        "date_of_birth",
        "is_male",
        "hometown",
        "additional_info",
        "phone_number",
        "is_kol",
        "created_at",
        "updated_at"
      ],
      "transform": {
        "mapping": {
          "id": {
            "type": "text",
            "copy_to": "id"
          },
          "full_name": {
            "type": "text",
            "copy_to": "full_name",
            "fields": {
              "ngram": {
                "type": "text",
                "analyzer": "vi_analyzer",
                "search_analyzer": "vi_analyzer",
                "fielddata": true
              }
            }
          },
          "national_id": {
            "type": "text",
            "copy_to": "national_id"
          },
          "citizen_id": {
            "type": "text",
            "copy_to": "citizen_id"
          },
          "image_url": {
            "type": "text",
            "copy_to": "image_url"
          },
          "date_of_birth": {
            "type": "date",
            "copy_to": "date_of_birth"
          },
          "is_male": {
            "type": "boolean",
            "copy_to": "is_male"
          },
          "hometown": {
            "type": "text",
            "copy_to": "hometown",
            "fields": {
              "ngram": {
                "type": "text",
                "analyzer": "vi_analyzer",
                "search_analyzer": "vi_analyzer",
                "fielddata": true
              }
            }
          },
          "additional_info": {
            "type": "text",
            "copy_to": "additional_info",
            "fields": {
              "ngram": {
                "type": "text",
                "analyzer": "vi_analyzer",
                "search_analyzer": "vi_analyzer",
                "fielddata": true
              }
            }
          },
          "phone_number": {
            "type": "text",
            "copy_to": "phone_number"
          },
          "is_kol": {
            "type": "boolean",
            "copy_to": "is_kol"
          },
          "created_at": {
            "type": "date",
            "copy_to": "created_at"
          },
          "updated_at": {
            "type": "date",
            "copy_to": "updated_at"
          }
        }
      }
    }
  },
